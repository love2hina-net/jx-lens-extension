{
  "type": "commonjs",
  "private": true,
  "name": "@love2hina/jx-lens-extension",
  "version": "0.0.1-alpha3",
  "description": "Lens extension to visualize Jenkins X resources and their relations",
  "author": "webmaster@love2hina.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/love2hina-net/jx-lens-extension.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16",
    "lens": "^6.5.0"
  },
  "renderer": "dist/renderer.js",
  "scripts": {
    "build": "npm run clean && npx webpack --config webpack.config.prod.ts",
    "dev": "npm run clean && npx webpack --config webpack.config.dev.ts",
    "macro": "npm run clean && npx tspc --build",
    "lint": "npx eslint . --ext ts,tsx",
    "clean": "npx rimraf dist"
  },
  "contributes": {
    "crds": [],
    "cloudProviders": [],
    "kubernetesDistros": []
  },
  "dependencies": {},
  "devDependencies": {
    "@k8slens/extensions": "^6.5.2",
    "@stylistic/eslint-plugin": "^1.5.3",
    "@types/eslint": "^8.56.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@types/react": "^17.0.74",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "node-gyp": "^10.0.1",
    "node-sass": "^9.0.0",
    "rimraf": "^5.0.5",
    "sass-loader": "^13.3.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "ts-macros": "file:ts-macros",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "asar": true,
    "files": [
      "dist"
    ]
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "k8slens",
    "lens",
    "extension",
    "jenkinsx"
  ]
}
